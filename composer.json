{
    "name": "dnadesign/silverstripe-responsiveimageset",
    "description": "Manually add responsive images to your site",
    "type": "silverstripe-vendormodule",
    "authors": [
        {
            "name": "Andrew Dover",
            "email": "andy.dover@dna.co.nz"
        }
    ],
    "require": {
        "silverstripe/framework": "^4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "dnadesign/silverstripe-elemental": ">=3.0",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "client"
        ]
    },
    "autoload": {
        "psr-4": {
            "DNADesign\\ResponsiveImageSet\\": "src/",
            "DNADesign\\ResponsiveImageSet\\Tests\\": "tests/php/"
        }
    }
}
